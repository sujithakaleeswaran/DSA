class Solution
{
    public int checksorted(int n,int i)
    {
    if(i>n)
    {
        return 0;
    }
    if(n%i==0)
    {
        System.out.print(i+" ");
    }
    return checksorted(n,i+1);
}
}
class Main {
    public static void main(String[] args) {
    Solution scan=new Solution();
    
    scan.checksorted(12,1);
    }
}

